Entity testies
Architecture Behavioral
//comen
Var
	A : in binary
	B : in binary[7,0] : '0'
	F : out binary
	C : binary : '1'
	E : binary[3,0]
	D : integer[0,15]
VarEnd

Logic parallel
	C = A And B
	F = Not C/* alas
	cositas
	AND2 A B C
	INV C F*/
	
	AND2 A B C
	INV C F
	
	D = (int) B
	E = (binary[4]) D
	
	Set F
	A if B = "00110011"
	C if A = '0'
	C if else
	
	Set F Switch D
	C case '0'|'1'
	A case '1'
	C case else
LogicEnd

Logic sequential
	If A = '0' then F = '1'
	else if B = "00110011" Then C = '0'
	else C = '1'
	
	Switch D
	F = '0' case 1
	F = '1' case 2
	F = '0' case else
	
	for i in E
		If i = 2 Then E[i] = '1'
		else E[i] = '0'
	next
LogicEnd
