salida : out binary[0,7]
entrada : binary[2,0]

entrada = c & b & a
Set salida Switch entrada
	"10000000" case "000"
	"01000000" case "001"
	"00100000" case "010"
	"00010000" case "011"
	"00001000" case "100"
	"00000100" case "101"
	"00000010" case "110"
	"00000001" case "111"