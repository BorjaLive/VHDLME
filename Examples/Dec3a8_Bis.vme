Entity Dec3a8

binary a, b, c, EN
binary[0,7] salida
binary[2,0] entrada
integer[0,7] entrada_int
binary[0,7] salida_en

entrada = c & b & a
entrada_int = (integer) entrada
Set salida_en Switch entrada_int
	"10000000" case 0
	"01000000" case 1
	"00100000" case 2
	"00010000" case 3
	"00001000" case 4
	"00000100" case 5
	"00000010" case 6
	"00000001" case 7
Set salida
	salida_en If EN = '1'
	"00000000" If else 